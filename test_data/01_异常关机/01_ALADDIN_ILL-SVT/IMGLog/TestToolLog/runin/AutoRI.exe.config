<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="dll" />
      <dependentAssembly>
        <assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="log4net" publicKeyToken="669e0ddf0bb1aa2a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.0.15.0" newVersion="2.0.15.0" />
      </dependentAssembly>
    </assemblyBinding>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="YamlDotNet" publicKeyToken="ec19458f3c15af5e" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <!--第一次启动运行自动调用Autoset mfg:TRUE启用，FALSE不启用-->
    <add key="AutoSetInRIBegin" value="TRUE" />
    <add key="AutoSetMode" value="mfg" />
    <add key="CheckTaskIgnored" value="TRUE" />
    <add key="SilentInTest" value="FALSE" />
    <!--服务器设定 ServerType:0 disable, 1-1 iTest(10.159.114.13:8086\V1), 1-2 Cloud(10.159.252.204:9200)-->
    <add key="ServerType" value="2" />
    <add key="ServerIpCheckList" value="{0}.{1}.1.1;{0}.{1}.1.4" />
    <!--iTest(runinimg01.itest.com)-->
    <add key="ITestServerIp" value="100.100.17.23"/>
    <add key="ITestServerPort" value="8088" />
    <add key="ITestServerVersion" value="V2"/>
    <add key="ITestHeartbeatTimeout" value="90000" />
    <!--UTP(10.159.252.202)-->
    <add key="UtpServerIp" value="100.100.230.1" />
    <add key="UtpWebPort" value="39202" />
    <add key="UtpBrokerPort" value="39201" />
    <add key="UtpMessageFequencyTimeout" value="60000" />
    <!--MES-->
    <add key="MesSwitch" value="FALSE" />
    <add key="MesServerIp" value="lmcp-service.lcfuturecenter.com" />
    <add key="MesEnv" value="prd" />
    <add key="MesServerPort" value="8096" />
    <add key="MesMercuryExWarehouseSwitch" value="TRUE"/>
    <add key="MesMercuryExWarehousePort" value="39203"/>
    <add key="MesVirtualStation" value="TRUE"/>
    <add key="MesVirtualStationPort" value="39204"/>
    <!--人脸识别防护开关(未完整调试)-->
    <add key="DetectFace" value="FALSE" />
    <!--BOM地址 Default="D:\TPDLCFG\BOM.BAT"-->
    <add key="BOMPath" value="D:\TPDLCFG\BOM.BAT" />
    <!--报错音量 1~100%-->
    <add key="VoiceVolume" value="100" />
    <!--FTP参数设定-->
    <add key="FtpUrlList" value="100.100.231.12" />
    <add key="FtpUserName" value="lclcfcrd" />
    <add key="FtpPassword" value="fclcfc(#$#" />
    <!--MFG 默认设置-->
    <add key="RuninShipFilePath" value="D:\Runin\Runin.log" />
    <add key="SharedRemoteNetworkPath" value="\\{0}\monitor\salog" />
    <add key="SharedRemoteUserName" value="lcswdl" />
    <add key="SharedRemotePassword" value="fcswdl" />
    <add key="SharedLocalNetworkPath" value="R:" />
    <add key="CompressionType" value="7z" />
    <add key="CompressionCommandLine" value="a {0} &quot;{1}&quot; {2} -r -mx=0 -m0=LZMA2 -ms=10m -mf=on -mhc=on -mmt=on" />
    <!--文件数据处理设置:开关(后续有新增只能做追加)-->
    <add key="FileDataHandle" value="TRUE" />
    <add key="FileDataHandleQmsJson" value="TRUE" />
    <add key="FileDataHandleMfgReport" value="TRUE" />
    <add key="FileDataHandleAutoLog" value="TRUE" />
    <add key="FileDataHandleModuleBSOD" value="FALSE" />
    <add key="FileDataHandleHealth" value="TRUE" />
    <!--文件数据处理设置:量产阶段(TRUE:按照上面配置的执行，FALSE:全部关闭，AUTO:默认Autolog不传，其他按照上面设置)-->
    <add key="FileDataHanleMpSwitch" value="AUTO" />
    <!--文件数据处理设置:上传文件数据通用接口,测试计划完成后执行(不录入数据库)。示例：CGTT@C:\Program Files (x86)\AutoRI\Config\TestModules\CGTT\logs-->
    <add key="FileDataHandleCommonPathList" value="" />
    <!--FTP 路径前缀,示例:SALOGTempBak\-->
    <add key="FtpPrePath" value="" />
    <!--RD 默认设置-->
    <add key="RDSharedIp" value="100.100.231.12" />
    <add key="RDSharedRemoteNetworkPath" value="\\{0}\Tools\TPDL\Monitor_Temp_Logs" />
    <add key="RDSharedRemoteUserName" value="lctpdl" />
    <add key="RDSharedRemotePassword" value="fclenovo@#123" />
    <add key="RDDefaultControl" value="TRUE" />
    <add key="RDDefaultCycles" value="1" />
    <add key="RDDefaultTimes" value="1" />
    <add key="RDPostRunDefaultPath" value="C:\tpdl\autocapture\autocapture.cmd" />
    <!--AutoRI调用控制参数(延迟单位秒)-->
    <add key="AutoRIDebugSwitch" value="FALSE" />
    <add key="AutoRIDelayCallTime" value="0" />
    <add key="AutoRIShowSplashScreen" value="TRUE" />
    <add key="AutoRIScrollEnable" value="FALSE" />
    <add key="AutoRIScrollCaption" value="We do what we say,We own what we do,We wow our customers 说到做到，尽心尽力，成就客户;Serving Our Customers 服务客户          Innovation 开拓创新          Entrepreneurship 创业精神          Teamwork with Intergrity and Trust 诚信共赢" />
    <add key="AutoRIExitKillList" value="bit;OSAgingTool;ThermalMonitor;ReportErrorContainer;PowerTestSx" />
    <add key="EnableXmlEncryptDecrypt" value="TRUE" />
    <add key="StatusAesKeyStr" value="" />
    <add key="ConfigAesKeyStr" value="" />
    <add key="BrightnessSaveValue" value="20"/>
    <add key="RawDataTypeIsList" value="DiskInfo;S3Resume;S4Resume;MsResume;WbBoot;CbBoot;FbBoot"/>
    <!--AutoRI 显示模式: normal, minimize, simplify-->
    <add key="AutoRIDisplayMode" value="normal" />
    <!--异常检测控制开关-->
    <add key="CheckAbnormalShutdown" value="TRUE" />
    <add key="CheckAbnormalShudownIgnorItem" value="PowerTestSx.exe:FB;PowerTestSx.exe:WB;PowerTestSx.exe:CB;"/>
    <add key="ParsingShutdownID" value="TRUE" />
    <add key="CheckEcShutdownID" value="TRUE"/>
    <add key="CheckGlobalReset" value="TRUE"/>
    <add key="GBLRST_CAUSE0" value="0xFE001924"/>
    <add key="GBLRST_CAUSE1" value="0xFE001928"/>
    <add key="GBLRST_CAUSE2" value="0xFE00192C"/>
    <add key="GBLRST_CAUSE0_REBOOT" value="10;11;12;16;24" />
    <add key="GBLRST_CAUSE1_REBOOT" value="0;2;10" />
    <add key="GBLRST_CAUSE2_REBOOT" value="9;10;13;17" />
    <add key="AMD_S5_RESET_STATUS" value="0xFED803C0"/>
    <add key="AMD_S5_RESET_STATUS_REBOOT" value="27"/>
    <add key="AbnormalRebootGapTime" value="50"/>
    <add key="Event41GapTime" value="60" />
    <add key="CheckBSODEvent" value="TRUE" />
    <add key="CheckCommonError" value="TRUE" />
    <!--测试过程中重启(非异常重启)-->
    <add key="CheckAbnormalRestart" value="FALSE" />
    <!--测试项倒计时,单位秒-->
    <add key="CountDownTime" value="15" />
    <!--WarnBoot延迟时间,单位秒-->
    <add key="WBDelayTime" value="10" />
    <!--ColdBoot 延迟及休眠时间(休眠时间不超过120s)，单位秒-->
    <add key="CBDelayTime" value="30" />
    <add key="CBSleepTime" value="30" />
    <!--设定厂区:一厂(P1),二厂(P2)-->
    <add key="FactoryLocation" value="P1" />
    <add key="GetPosition" value="TRUE" />
    <!--时间同步标记年-->
    <add key="SyncTimeFlagYear" value="2030" />
    <!--测试分类信息-->
    <add key="TaskTypeCategory" value="PowerTestSx.exe:SxTest;BurnIn9Test.exe:BurnIn9;Futuremark_3DMark.exe:3DMark;ThermalTest(TAT).exe:ThermalTest;ThermalTest(TCT).exe:ThermalTest;CGTT.exe:CGTT;CGTT.Console.exe:CGTT;LDiag.exe:Ldiag;Mods.exe:Mods;DiskInfo.exe:DiskInfo;VRAMCheckTool.exe:VramCheck;" />
    <!--事件信息的分类获取-->
    <add key="FilterByErrorCode" value="TRUE" />
    <add key="FilterByTime" value="TRUE" />
    <add key="CheckAdapterPower" value="FALSE"/>
    <!-- 是否每次重启都进行检查-->
    <add key="CheckAdapterPowerAppStart" value="FALSE"/>
    <add key="CheckAdapterPowerLowestDefault" value="230"/>
    <!-- 示例:GX4A0:130;YH560:330, 匹配不到就使用default值 -->
    <add key="CheckAdapterPowerLowestSet" value=""/>
    <add key="CheckAdapterPowerTimes" value="5"/>
    <add key="CheckAdapterPowerKeepPassTimes" value="2"/>
    <!-- 默认空则检查所有测试项目,填测试项名称在对应测试项开始前检查,多个以分号隔开 -->
    <add key="CheckAdapterPowerPoint" value=""/>
  </appSettings>
</configuration>